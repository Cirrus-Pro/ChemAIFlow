\\\"Prot_ResidueLength\\\",\\\"Prot_MolecularWeight_Da\\\",\\\"Prot_GRAVY\\\",\
\\\"pIC50\\\"};\n   safeNum[x_] := Which[NumericQ[x],N[x], \
StringQ[x]&&StringMatchQ[x,NumberString], \
Quiet@Check[N@ToExpression[x],Missing[\\\"Bad\\\"]], True, x];\n   \
CleanDataset[d_List] := \
